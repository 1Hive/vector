  "main": "index.js",
  "scripts": {
    "build": "prisma generate && npm run compile-schemas && rm -rf dist && tsc -p tsconfig.json",
    "start": "prisma migrate up --experimental && node dist/index.js",
    "test": "prisma generate && prisma migrate up --experimental && ts-mocha --bail --check-leaks --exit --timeout 60000 'src/**/*.spec.ts'",
